# OpsMedic example environment variables
# Copy this file to .env and fill in your API keys and endpoints.

# Active LLM provider: gemini | llama | fastrouter
ACTIVE_LLM_PROVIDER=gemini

# Gemini (Google) API key
GEMINI_API_KEY=

# Llama (self-hosted) settings
LLAMA_API_KEY=
LLAMA_ENDPOINT=

# FastRouter settings
FASTRTR_API_KEY=
FASTRTR_ENDPOINT=https://go.fastrouter.ai/api/v1

# Optional: override agent port
OPS_AGENT_PORT=8001
